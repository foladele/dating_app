
<%= form_for @person, html: { multipart: true } do |f| %>

	<%= f.label :image%>
	<%= f.file_field :image %>
	<%= f.label :name%>
	<%= f.text_field :name %>
	<%= f.label :age %>
	<%= f.text_field :age %>
	<%= f.label :gender %>
	<%= f.text_field :gender %>
	<%= f.hidden_field :user_id, :value => @user%>
	<%= f.submit %>
<%end%>